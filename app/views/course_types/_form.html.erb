<input type="hidden" name="authenticity_token" 
 value="<%= form_authenticity_token %>">
<div class="form-group"> 
  <label class="col-md-3 control-label" for="course_type_long_name">Long Name</label>
    <div class="col-md-4">
      <input type="text" id="course_type_long_name" 
             name="course_type[long_name]" 
             value="<%= @course_type.long_name %>"
             class="form-control">
  </div>
</div>
<div class="form-group"> 
  <label class="col-md-3 control-label" for="course_type_short_name">Short Name</label>
  <div class="col-md-4">
    <input type="text" id="course_type_short_name" 
           name="course_type[short_name]" 
           value="<%= @course_type.short_name %>"
           class="form-control">
  </div>
</div>
<div class="form-group">
  <label class="col-md-3 control-label" for="course_type_discipline">Discipline</label>
  <div class="col-md-3">
    <input type="text" id="course_type_discipline" 
           name="course_type[discipline]" 
           value="<%= @course_type.discipline %>"
           class="form-control">
  </div>
</div>
<div class="form-group">
  <label class="col-md-3 control-label" for="course_type_level">Level</label>
  <div class="col-md-2">
    <input type="text" id="course_type_level" 
           name="course_type[level]" value="<%= @course_type.level %>"
           class="form-control">
  </div>
</div>
<div class="form-group">
  <label class="col-md-3 control-label sr-only"></label>
  <div class="col-md-4">
    <input type="submit" value="Submit" class="btn btn-primary">