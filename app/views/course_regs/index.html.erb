
<%= render partial: "form" %>
<%= render partial: "force_enroll_form" %>
<br>
<br>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Student</th>
      <th>Course</th>
      <th>Date</th>
      <th>Registerer</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @course_regs.each do |course_reg| %>
    <tr>
      <td><%= course_reg.student.name %></td>
      <td><%= course_reg.course.dept_code %></td>
      <td><%= course_reg.created_at %></td>
      <td><%= course_reg.user.email %></td>
      <td>
        <%= link_to "delete", course_reg_url(course_reg), :method => "delete" %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>