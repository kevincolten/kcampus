<form action="<%= new_user_url %>" method="post">
  <p>
  <label for="user_fname">First Name</label>
  <input type="text" id="user_fname" name="user[fname]">
  </p>
  <p>
  <label for="user_lname">Last Name</label>
  <input type="text" id="user_lname" name="user[lname]">
  </p>
  <p>
  <label for="user_email">Email</label>
  <input type="text" id="user_email" name="user[email]">
  </p>
  <p>
  <label for="user_password">Password</label>
  <input type="text" id="user_password" name="user[password]">
  </p>
  <input type="submit" value="Submit">
</form>