<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Year</th>
      <th>Semester</th>
      <th>Code</th>
      <th>Department Code</th>
      <th>Registration</th>
      <th>Start-End</th>
      <th>Drop Date</th>
      <th></th>
     </tr>
   </thead>
   <tbody>
     <% @terms.each do |term| %>
       <tr>
         <td><%= term.year %></td>
         <td><%= term.semester %></td>
         <td><%= term.code %></td>
         <td><%= term.dept_code %></td>
         <td><%= "#{term.reg_start.strftime("%m/%d/%Y")} - #{term.reg_end.strftime("%m/%d/%Y")}" %>     
       </tr>
     <% end %>
   </tbody>
  </table>